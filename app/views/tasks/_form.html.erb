<%= form_for([@course, @task]) do |f| %>
    <fieldset>
        <div class="field">
            <%= f.label :name %><br>
            <%= f.text_field :name %><br>
        </div>
        <div class="field">
            <%= f.label :due_date %><br>
            <%= f.text_field :due_date %><br>
        </div>
        <br>
        <div class="actions">
            <%= f.submit %>
        </div>
    </fieldset>
<% end %>